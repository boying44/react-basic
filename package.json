{
  "name": "react-redux-express",
  "version": "1.0.0",
  "description": "A template a for React-Redux app running on Express",
  "main": "app.js",
  "scripts": {
    "start": "webpack --config webpack.dev.js --display-error-details && node backend.js",
    "nodemon": "nodemon --inspect -e ts,pug,JSON,css, backend.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "tslint .",
    "fix": "tslint --fix .",
    "css": "tcm src/",
    "tsc": "tsc src/app.tsx --jsx react"
  },
  "repository": "https://github.com/boying44/react-redux-express",
  "author": "boying44",
  "license": "MIT",
  "dependencies": {
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "6.0.11",
    "@types/webpack": "4.4.22",
    "@types/webpack-env": "^1.13.6",
    "babel-register": "^6.26.0",
    "css-loader": "^2.0.2",
    "express": "^4.14.0",
    "pug": "^2.0.0-beta6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "typescript": "3.2.2",
    "webpack": "4.28.1",
    "webpack-merge": "^4.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.5.0",
    "redux-devtools-extension": "^2.13.7",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typed-css-modules": "^0.3.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11"
  }
}
